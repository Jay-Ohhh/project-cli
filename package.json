{
  "name": "rolib-cli",
  "version": "1.1.5",
  "description": "A rollup cli: create a dev-environment includes Rollup, TypeScrit, Babel, Git, ESlint, Prettier...",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/src/index.d.ts",
  "bin": {
    "rolib": "./bin/index.js"
  },
  "homepage": "https://github.com/Jay-Ohhh/rolib-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jay-Ohhh/rolib-cli.git"
  },
  "bugs": {
    "url": "https://github.com/Jay-Ohhh/rolib-cli/issues"
  },
  "keywords": [
    "cli",
    "Rollup",
    "TypeScript"
  ],
  "author": "Jay_Ohhh",
  "license": "MIT",
  "files": [
    "dist",
    "bin"
  ],
  "lint-staged": {
    "src/**": "npm run lint"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development rollup -c rollup.config.ts --configPlugin typescript2 -w",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.ts --configPlugin typescript2",
    "dev": "nodemon -x ts-node --files src/index.ts",
    "lint": "eslint src --fix --ext .js,.ts",
    "build:types": "tsc --emitDeclarationOnly --noEmit false",
    "commit": "cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.17.2",
    "chalk": "^4.0.0",
    "cli-spinners": "^2.6.1",
    "commander": "^9.0.0",
    "console-clear": "^1.1.1",
    "download": "^8.0.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.1",
    "handlebars": "^4.7.7",
    "inquirer": "^8.2.0",
    "log-symbols": "^4.1.0",
    "ora": "^5.4.1",
    "shelljs": "^0.8.5",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/download": "^8.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.0",
    "@types/node": "^17.0.21",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/parser": "^5.12.1",
    "commitizen": "^4.2.4",
    "commitlint-config-git-commit-emoji": "^1.0.0",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "conventional-changelog-emoji-config": "^1.4.8",
    "core-js": "^3.21.1",
    "cross-env": "^7.0.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fuzzy": "^0.1.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rollup": "^2.68.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "semantic-release": "^19.0.2",
    "standard-version": "^9.3.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.13.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
